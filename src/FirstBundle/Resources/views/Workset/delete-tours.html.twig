{# Workset/Delete #}

{% extends "FirstBundle:Layouts:layout.html.twig" %}

{% block title %}
    Delete All Workset's Tours

{% endblock  %}

{% block content %}
    <div class="container">
        <div class="row">

            <div class="col-md-12">
                <h2 class="danger">

                    You sure you want to Delete All Tours ?
                </h2>
                <h3>All data for this workset will be lost</h3>
            </div>

        </div>
        <div class="row">

            <div class="col-md-12">
                <form role="form" method="POST">
                    <button type="submit" class="btn btn-danger">Supprimer les Tours !</button>
                </form>
            </div>

        </div>
    </div>
{% endblock  %}

